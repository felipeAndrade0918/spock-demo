{
  "request": {
    "urlPath": "/search",
    "method": "GET",
    "queryParameters": {
      "format": {
        "equalTo": "json"
      },
      "resources": {
        "equalTo": "game"
      },
      "query": {
        "equalTo": "starfox"
      },
      "api_key": {
        "matches": ".*"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "body-with-results.json",
    "headers": {
      "Content-Type": "application/json;charset=UTF-8"
    }
  }
}
